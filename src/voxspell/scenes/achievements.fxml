<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox fx:id="vBox" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="voxspell.scenes.controllers.AchievementsController">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label fx:id="title" style="-fx-font-family: &quot;Comic Sans MS&quot;; -fx-font-size: 48;"
                   text="Achievements">
               <font>
                  <Font name="Comic Sans MS" size="48.0" />
               </font>
            </Label>
         </children>
      </HBox>
       <HBox fx:id="hBox" prefHeight="175.0" prefWidth="900.0" spacing="20.0">
           <padding>
              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
           </padding>
       </HBox>
      <VBox prefHeight="200.0" prefWidth="100.0"/>
      <HBox alignment="BOTTOM_RIGHT" prefHeight="62.0" prefWidth="900.0">
         <children>
            <Button fx:id="mainMenuButton" mnemonicParsing="false" prefHeight="62.0" prefWidth="128.0" style="-fx-background-color: b6e7c9;" text="Go Back">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="50.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
